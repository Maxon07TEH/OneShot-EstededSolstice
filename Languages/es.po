msgid "@niko um....what?"
msgstr "@niko Mmm... ¿Qué?"

#: Maps/Blue/Dormitories/EV523:3:1:text
msgid "@niko_speak So you're just gonna...follow me?"
msgstr "@niko_speak ¿Así que vas a... seguirme?"

#: Maps/Blue/Dormitories/EV523:3:2:text Maps/Blue/Dormitories/118 - robot voice/Map Events:2:9:text
msgid "[YES.]"
msgstr "[SÍ.]"

#: Maps/Blue/Dormitories/EV523:3:3:text
msgid "[UNTIL YOU SAFELY REACH A BUILDING OF YOUR DESTINATION.]"
msgstr "[HASTA QUE LLEGUES SANO Y SALVO A TU DESTINO.]"

#: Maps/Blue/Dormitories/EV523:3:4:text
msgid "@niko Alright."
msgstr "@niko Muy bien."

