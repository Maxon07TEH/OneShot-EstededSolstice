msgstr "@niko6 Eu acordei pela primeira vez neste quarto..."

#: Maps/FINALE/Last room/EV003:1:60:text
msgid "@niko_speak \\p? Are you still here?"
msgstr "@niko_speak \\p? Ainda está aqui?"

#: Maps/FINALE/Last room/EV003:1:62:text
msgid "@niko2 ...\\p?"
msgstr "@niko2 ...\\p?"

#: Maps/FINALE/Last room/EV003:1:64:choice:0 Maps/Tower/Sunroom/lightbulb:1:16:choice:0
